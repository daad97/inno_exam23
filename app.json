{
  "expo": {
    "name": "JournalApp",
    "slug": "JournalApp",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Appen bruger dine billeder til at tilf√∏je dem i din dagbog.",
          "cameraPermission": "Tillad at $(PRODUCT_NAME) bruger dit kamera til at tage billeder til din dagbog."
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
